<!doctype html
<html
  <head
    </meta
      @charset UTF-8
    <title
      RALLY
    <link
      @rel stylesheet
      @type text/css
      @href /index.css
  <body
    <div
      @id app
      <h1
        @class title
        @v-on:click click
        <p
          @class message
          {{message}}
        <p
          @class stamp-card-id
          {{stampCardId}}
      <my-stamp-rally
        @v-bind:stamp-rally stampRally
      <p
        @class description
        This is unofficial RALLY client.
    <script
      @type text/javascript
      @src /index.js
