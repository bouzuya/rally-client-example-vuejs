<div
  @class stamp-rally
  <div
    @class rally
    <a
      @v-bind:href stampRallyUrl
      @class link
      @target _blank
      R
  <div
    @class name
    @v-text stampRallyName
  <div
    @class tagline
    @v-html stampRallyTagline
  <div
    @class description
    @v-html stampRallyDescription
  <div
    @class image
    </img
      @width 64
      @height 64
      @v-bind:src stampRallyImage
